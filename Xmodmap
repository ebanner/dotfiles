! Modifications to the dvorak layout

! Number Row
keycode  49 = BackSpace
keycode  10 = exclam 1
keycode  11 = at 2
keycode  12 = numbersign 3
keycode  13 = dollar 4
keycode  14 = percent 5
keycode  15 = asciicircum 6
keycode  16 = ampersand 7
keycode  17 = asterisk 8
keycode  18 = parenleft 9
keycode  19 = parenright 0
keycode  20 = minus underscore
keycode  21 = equal plus
keycode  22 = grave asciitilde

! Top Row
keycode  24 = comma less
keycode  25 = period greater
keycode  26 = z Z
keycode  27 = p P
keycode  28 = y Y
keycode  29 = backslash bar
keycode  30 = f F
keycode  31 = g G
keycode  32 = c C
keycode  33 = r R
keycode  34 = l L
keycode  35 = bracketleft braceleft
keycode  51 = bracketright braceright

! Middle Row

! Make caps lock a Control key
remove Lock = Caps_Lock
keycode  66 = Control_L
add Control = Control_L

keycode  38 = a A
keycode  39 = o O
keycode  40 = e E
keycode  41 = u U
keycode  42 = i I
keycode  43 = apostrophe quotedbl
keycode  44 = d D
keycode  45 = h H
keycode  46 = t T
keycode  47 = n N
keycode  48 = s S

! Bottom Row
keycode  52 = semicolon colon
keycode  53 = q Q
keycode  54 = j J
keycode  55 = k K
keycode  56 = x X
keycode  57 = slash question
keycode  58 = b B
keycode  59 = m M
keycode  60 = w W
keycode  61 = v V
