! Left hand number hack
keycode  11 = Tab at
keycode  12 = 3 numbersign
keycode  13 = 4 dollar

! Right hand number hack
keycode  16 = 7 ampersand
keycode  17 = 8 asterisk
keycode  18 = Return Return
keycode  19 = parenleft parenright

! Caps Lock
keycode  66 = Escape
clear Lock

! Make up for number hack inconveniences
keycode  67 = 2
keycode  68 = at
keycode  74 = 9
keycode  75 = 0
